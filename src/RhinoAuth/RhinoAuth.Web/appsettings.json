{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "AppLimits": {
        "RegistrationAttemptLimit": 100,
        "IpAttemptLimit": 10,
        "PhoneNumberAttemptLimit": 10,
        "AllowUnsafeNetworks": false,
        "CodeRetryLimit": 3,
        "CodeExpirationInMinute": 5,
        "UnusedCodesLimit": 5,
        "WrongPasswordLimit": 4,
        "AccountLockoutInMinute": 15,
        "ResetPasswordLimitPerDay": 1,
        "ChangeProfileLimitPerDay": 10,
        "ChangePhoneNumberLimitPerDay": 1,
        "ChangeEmailLimitPerDay": 1,
        "UnfinishedOAuthRequestLimitPerHour": 10
    },
    "AppBehavior": {
        "PublishEvents": false,
        "UseRevokedTokensCache": false,
        "OAuthIssuer": "https://local-server"
    }
}
